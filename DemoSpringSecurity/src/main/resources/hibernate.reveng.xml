<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="springsecurity" match-name="APP_ROLE"/>
  <table-filter match-catalog="springsecurity" match-name="APP_USER"/>
  <table-filter match-catalog="springsecurity" match-name="Persistent_Logins"/>
  <table-filter match-catalog="springsecurity" match-name="USER_ROLE"/>
</hibernate-reverse-engineering>